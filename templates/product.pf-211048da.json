                "custom_liquid_BnpwyB": {
                  "type": "custom-liquid",
                  "name": "Sale Countdown Timer",
                  "settings": {
                    "custom_liquid": "<!DOCTYPE html>\n<html lang=\"en\">\n<head>\n    <meta charset=\"UTF-8\">\n    <meta name=\"viewport\" content=\"width=device-width, initial-scale=1.0\">\n    <style>\n        @keyframes wave {\n            0% {\n                background-position: 0 0;\n            }\n            50% {\n                background-position: 100% 0;\n            }\n            100% {\n                background-position: 0 0;\n            }\n        }\n\n        .countdown-bar {\n            display: flex;\n            justify-content: center;\n            align-items: center;\n            background: linear-gradient(to right, #000000, #4770DB);\n            background-size: 200% 100%;\n            animation: wave 10s ease infinite;\n            padding: 5px; /* Reduced padding to make the block smaller */\n            font-family: Arial, sans-serif;\n            font-size: 14px;\n            font-weight: bold;\n            text-transform: uppercase;\n            color: white;\n        }\n\n        .countdown-container {\n            display: flex;\n            align-items: center;\n            justify-content: center; /* Center text and timer horizontally */\n            width: 100%;\n            max-width: 900px;\n            margin: 0 auto;\n            gap: 5px; /* Reduced gap between text and timer */\n        }\n\n        .countdown-text {\n            color: white;\n            margin-right: 5px; /* Reduced margin for smaller block */\n            font-size: 13px; /* Reduced font size */\n        }\n\n        .countdown {\n            display: flex;\n            align-items: center;\n            gap: 2px; /* Reduced gap between time sections */\n        }\n\n        .time-section {\n            display: flex;\n            align-items: center;\n            justify-content: center;\n            background-color: #ffffff;\n            color: #000000;\n            width: 30px; /* Reduced width for smaller block */\n            height: 30px; /* Reduced height for smaller block */\n            margin: 0;\n            border-radius: 5px;\n            font-weight: bold;\n            font-size: 14px; /* Adjusted font size */\n        }\n\n        .time-label {\n            font-size: 8px; /* Reduced font size */\n            margin-left: 2px;\n            margin-right: 2px;\n        }\n\n        /* Adjust layout for desktop screens */\n        @media (min-width: 1024px) {\n            .countdown-container {\n                gap: 5px; /* Maintain smaller gap for desktop */\n            }\n\n            .countdown-text {\n                margin-right: 5px; /* Reduced margin on desktop */\n                font-size: 12px; /* Ensure text size is consistent */\n            }\n        }\n    </style>\n    <title>Countdown Timer</title>\n</head>\n<body>\n\n<div class=\"countdown-bar\">\n    <div class=\"countdown-container\">\n        <div class=\"countdown-text\">üè∑Ô∏è GET 40% OFF! + FREE SHIPPING‚ú® ENDS IN </div>\n        <div id=\"countdown\" class=\"countdown\">\n            <div class=\"time-section\" id=\"hours\">00</div><div class=\"time-label\">HRS</div>\n            <div class=\"time-section\" id=\"minutes\">00</div><div class=\"time-label\">MIN</div>\n            <div class=\"time-section\" id=\"seconds\">00</div><div class=\"time-label\">SEC</div>\n        </div>\n    </div>\n</div>\n\n<script>\n    function startCountdown() {\n        let countdownDuration = 29 * 60; // 29 minutes in seconds\n        let countdownEnd = parseInt(localStorage.getItem('countdownEnd'));\n        \n        // If there's no previous countdownEnd, set it to now + countdownDuration\n        if (!countdownEnd || countdownEnd < Date.now()) {\n            countdownEnd = Date.now() + countdownDuration * 1000;\n            localStorage.setItem('countdownEnd', countdownEnd);\n        }\n        \n        const x = setInterval(function() {\n            const now = Date.now();\n            const distance = countdownEnd - now;\n\n            const hours = Math.floor((distance % (1000 * 60 * 60 * 24)) / (1000 * 60 * 60));\n            const minutes = Math.floor((distance % (1000 * 60 * 60)) / (1000 * 60));\n            const seconds = Math.floor((distance % (1000 * 60)) / 1000);\n\n            document.getElementById(\"hours\").innerHTML = (hours < 10 ? \"0\" : \"\") + hours;\n            document.getElementById(\"minutes\").innerHTML = (minutes < 10 ? \"0\" : \"\") + minutes;\n            document.getElementById(\"seconds\").innerHTML = (seconds < 10 ? \"0\" : \"\") + seconds;\n\n            if (distance < 0) {\n                clearInterval(x);\n                localStorage.setItem('countdownEnd', Date.now() + countdownDuration * 1000);\n                startCountdown(); // Restart the countdown\n            }\n        }, 1000);\n    }\n\n    startCountdown();\n</script>\n\n</body>\n</html>"
